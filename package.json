  ],
  "engines": {
    "node": "18 || 20 || 21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/metacalc.git"
  },
  "bugs": {
    "url": "https://github.com/metarhia/metacalc/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "homepage": "https://metarhia.com",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/tshemsedinov"
  },
  "scripts": {
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "types": "tsc -p tsconfig.json",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "test": "npm run lint && npm run types && metatests test/unit.js"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "eslint": "^8.44.0",
    "eslint-config-metarhia": "^8.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "metatests": "^0.8.2",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
